
<h1>Index Page</h1>

<% if @questions %>
<% @questions.each do |q| %>
 	<ul>
		<li>Question: <%= q.description %></li>
		<li>Author: <%= q.user.username %></li>
		<li>Created at: <%= q.created_at %></li>
		<li><a href="/question/<%=q.id%>"> Reply </a></li>
	</ul>
	<% end %>	
<% end %>

<p><a href="/new">Create Question</a></p>



